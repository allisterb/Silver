  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net461|AnyCPU'">
    <Optimize>False</Optimize>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="*.*" />
	
    <Compile Include="UintBase.cs" />
    <Compile Include="Uint128.cs" />
    <Compile Include="Uint256.cs" />
    <Compile Include="Address.cs"/>
    <Compile Include="IBlock.cs"/>
    <Compile Include="Block.cs"/>
    <Compile Include="DeployAttribute.cs"/>
    <Compile Include="IContractLogger.cs" />
    <Compile Include="ICreateResult.cs" />
    <Compile Include="IInternalHashHelper.cs" />
    <Compile Include="ISmartContractState.cs" />
    <Compile Include="ITransferResult.cs" />
    <Compile Include="IInternalTransactionExecutor.cs" />
    <Compile Include="IMessage.cs" />
    <Compile Include="IndexAttribute.cs" />
    <Compile Include="IPersistentState.cs" />
    <Compile Include="ISerializer.cs" />
    <Compile Include="Message.cs" />
  </ItemGroup>
  

  
  <ItemGroup>
    <Reference Include="System.Compiler.Runtime">
      <HintPath>..\..\lib\net461\System.Compiler.Runtime.dll</HintPath>
    </Reference>
    <Reference Include="System.Numerics"></Reference>
  </ItemGroup>
  
</Project>
